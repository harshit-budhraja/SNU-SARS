<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#fff8f9fa" />
            <stroke android:width="1.0dip" android:color="#ffd2d2d2" />
            <corners android:radius="1.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#fff8f9fa" android:endColor="#ffd2d2d2" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ffd2d2d2" />
            <corners android:radius="1.0dip" />
        </shape>
    </item>
</selector>